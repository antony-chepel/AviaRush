<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/av_rush_bg"
    tools:context=".moicxuhd.Hquierodijc">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/plan_av_rush" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Avia Rush Game Presents"
        android:textSize="20sp"
        android:fontFamily="@font/av_rush_bold"
        android:textStyle="bold"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.0" />

    <EditText
        android:id="@+id/ovckjivcb"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_av_res"
        android:ems="10"
        android:hint="Name"
        android:inputType="text"
        android:maxLength="20"
        android:padding="18dp"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView4"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="230dp"
        android:layout_height="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.100000024"
        app:srcCompat="@drawable/tv_name_av_rush_text" />

    <Button
        android:id="@+id/qowijids"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:text="Lets Go"
        android:textStyle="bold"
        android:fontFamily="@font/av_rush_reg"
        android:backgroundTint="@color/prim_av_rush_b"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ovckjivcb"
        app:layout_constraintVertical_bias="0.22000003" />

    <Button
        android:id="@+id/button3"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:backgroundTint="@color/prim_av_rush_b"
        android:fontFamily="@font/av_rush_reg"
        android:text="Exit"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/qowijids"
        app:layout_constraintVertical_bias="0.35000002" />
    <TextView
        android:id="@+id/ofofkcjicxi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="3dp"
        android:fontFamily="@font/av_rush_reg"
        android:text="Enter your real name to continue"
        android:textColor="@color/clr_eror_rush"
        android:textSize="12sp"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@+id/ovckjivcb"
        app:layout_constraintStart_toStartOf="@+id/ovckjivcb"
        app:layout_constraintTop_toBottomOf="@+id/ovckjivcb" />

    <ImageView
        android:id="@+id/mkcoxoxcixcji"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/av_rush_sett" />

</androidx.constraintlayout.widget.ConstraintLayout>