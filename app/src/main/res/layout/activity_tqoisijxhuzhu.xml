<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/av_rush_bg"
    android:layout_height="match_parent"
    tools:context=".moicxuhd.Tqoisijxhuzhu">

    <ImageView
        android:id="@+id/im_main"
        android:layout_width="250dp"
        android:layout_height="250dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.22"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.26999998"
        app:srcCompat="@drawable/plan_av_rush" />

    <ProgressBar
        android:id="@+id/ovkbv"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="60dp"
        android:layout_height="250dp"
        android:progressDrawable="@drawable/pb_av_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.92"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.26" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:textSize="23sp"
        android:fontFamily="@font/av_rush_bold"
        android:text="Tap on plane for setting engine power"
        app:layout_constraintBottom_toTopOf="@+id/im_main"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/iocxjixji"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/av_rush_reg"
        android:text="Avia Bonus Scores : x100"
        android:textColor="@color/white"
        android:textSize="23sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button2"
        app:layout_constraintVertical_bias="0.25" />

    <Button
        android:id="@+id/button2"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:textStyle="bold"
        android:backgroundTint="@color/prim_av_rush_b"
        android:fontFamily="@font/av_rush_reg"
        android:text="Lets Fly"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/im_main"
        app:layout_constraintVertical_bias="0.22000003" />

    <TextView
        android:id="@+id/odijf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textStyle="bold"
        android:layout_marginTop="4dp"
        android:text="0"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/ovkbv"
        app:layout_constraintStart_toStartOf="@+id/ovkbv"
        app:layout_constraintTop_toBottomOf="@+id/ovkbv" />
</androidx.constraintlayout.widget.ConstraintLayout>